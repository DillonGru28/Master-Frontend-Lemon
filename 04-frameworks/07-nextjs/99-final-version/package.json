{
  "name": "07-nextjs-examples",
  "version": "1.0.0",
  "description": "Nextjs examples",
  "scripts": {
    "start": "run-p -l start:dev start:api-server",
    "start:dev": "next dev",
    "start:debug": "run-p -l debug start:api-server",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
