    "axios": "^0.21.1",
    "emotion-server": "^11.0.0",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.2.0",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.5"
  }
